output$bar <- renderPlot({
bar1 <- tapply(data[,input$yaxis], data$State, mean)
barplot(bar1, col = input$colour, xlab = "State",
main = "First visualization")
})
output$bar2 <- renderPlot({
bar <- tapply(poverty[,input$yaxis2], poverty$State, mean)
barplot(bar, col= input$colour2, xlab = "State", ylab = "Poverty rate",
main = "Second visualization")
})
}
